datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


generator client {
  provider = "prisma-client-js"
}


model User{
id String @id @db.VarChar(20)
password String @db.Char(64)
email String @unique @db.VarChar(50)
nickname String @unique @db.VarChar(50)
}

